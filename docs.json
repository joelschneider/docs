{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mooncamp",
  "colors": {
    "primary": "#383630",
    "light": "#737373",
    "dark": "#171717"
  },
  "background": {
    "color": {
      "light": "#FBF7F3"
    }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "languages": [
      {
        "language": "en",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["introduction", "quickstart", "development"]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "language": "de",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["introduction", "quickstart", "development"]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Support",
          "href": "https://mintlify.com/docs",
          "icon": "headset"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@mooncamp.com"
      }
    ]
  },
  "footer": {
    "socials": {
      "linkedin": "https://linkedin.com/company/mooncamp"
    }
  }
}
